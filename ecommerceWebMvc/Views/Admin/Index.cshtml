@model IEnumerable<ecommerceWebMvc.Models.Classes.ApplicationUser>
@{
    ViewBag.Title = "Index";
   
}


<!-- End Page Title -->

<div class="panel panel-primary">
    <div class="panel-heading">
        Kullanıcılar

    </div>
    <table class="table table-striped">
        <tr>
            <td>Id</td>
            <td>Kullanıcı Adı</td>
            <td>Email</td>


        </tr>
        @if (Model.Count() == 0)
        {
            <tr>
                <td colspan="3" class="text-center">Kullanıcı Yok</td>
            </tr>
        }
        else
        {
            foreach (var item in Model)
            {
                <tr>
                    <td>@item.Id</td>
                    <td>@item.UserName</td>
                    <td>@item.Email</td>
                </tr>
            }

        }
    </table>



</div>
